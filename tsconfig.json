{
    "compilerOptions": {
      "baseUrl": "./",
      "target": "es5",
      "module": "ESNext",
      "allowJs": true,
      "declaration": true,
      "noEmit": true,
      "strict": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
    }
  }